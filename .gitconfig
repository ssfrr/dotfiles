[user]
	name = Spencer Russell
	email = srussell@bulogics.com
[push]
	default = current
[color]
	diff = auto
	status = auto
	branch = auto
	interactive = auto
[core]
	autocrlf = false
	excludesfile = /Users/srussell/.gitignore_global
[merge] 
      tool = vimdiff
[diff]
      tool = vimdiff
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[alias]
   tree = log --all --graph --decorate --pretty=oneline --abbrev-commit
